config dedalo
    option disabled 1
    option network 'network_addr/netmask'
    option splash_page 'http://host[:port]/wings'
    option aaa_url 'http[s]://host[:port]/wax/aaa'
    option api_url 'http[s]://host[:port]/api'
    option hotspot_id 'hotspot-id'
    option unit_name 'unit-id'
    option unit_description 'unit-description'
    option unit_uuid 'generated-uuid'
    option secret 'your-secret'
    option allow_origin '*'
